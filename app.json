{
	"expo": {
		"name": "Melios",
		"slug": "melios",
		"version": "1.1.1",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "app",
		"userInterfaceStyle": "automatic",
		"notification": { "icon": "./assets/images/icon.png" },
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"versionCode": 6,
			"icon": "./assets/images/icon.png",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.raphplt.melios",
			"permissions": ["NOTIFICATIONS"],
			"googleServicesFile": "./google-service-account-file/melios-4a03d-44ca4cc6331d.json"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router", "expo-secure-store"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "02832e16-a46a-43ec-8a0a-1b271300170e"
			}
		}
	}
}
